^(.+)er$ -> $1 {stamm-pr5} 	    # assiég(ez)

[
A_apiecer
]
^(.+)é(.)er$ -> $1è$2 {stamm-pr1} 	# apièc(e)
^(.+)cer$ -> $1ç {stamm-pr4} 		# apiéç(ons)
^(.+)er$ -> $1 {stamm-pr5} 			# apiéc(ez)


