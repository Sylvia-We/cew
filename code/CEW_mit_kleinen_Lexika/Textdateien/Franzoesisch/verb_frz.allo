
##### 1 Präsensstamm #####

[
A_chanter
]
^(.+)er$ -> $1 {stamm-pr4} 		# chant(e)

[
A_rompre
]
^(.+)re$ -> $1 {stamm-pr4}  		# romp(s)

[
A_vendre
]
^(.+)re$ -> $1 {stamm-pr4}  		# vend(s)
^(.+)re$ -> $1 {3 Sg}

##### 2 Präsensstämme #####

# Opposition 1,2,3,6 - 4,5 #

[
A_appeler
]
^(.+)(.)er$ -> $1$2$2 {stamm-pr1} 		# appell(e)
^(.+)er$ -> $1 {stamm-pr4} 		# appel(ons)

[
A_appuyer
]
^(.+)yer$ -> $1i {stamm-pr1} 		# appui(e)
^(.+)er$ -> $1 {stamm-pr4} 		# appuy(ons)

[
A_lever
]
^(.+)e(.*?)er$ -> $1è$2 {stamm-pr1}	 	# lèv(e)
^(.+)er$ -> $1 {stamm-pr4} 			# lev(ons)

[
A_preferer
]
^(.+)é(.*?)er$ -> $1è$2 {stamm-pr1} 	# préfèr(e)
^(.+)er$ -> $1 {stamm-pr4} 			# préfèr(ons), préfèr(ons)


# Opposition 1,2,3 - 4,5,6 #

[
A_sortir
]
^(.+)tir$ -> $1 {stamm-pr1} 		# sor(s)
^(.+)ir$ -> $1 {stamm-pr4} 		# sort(ons)

[
A_battre
]
^(.+)tre$ -> $1 {stamm-pr1} # bat(s)
^(.+)re$ -> $1 {stamm-pr4} # batt(ons)
^(.+)tre$ -> $1 {3 Sg}


# Opposition 1,2,3,5,6 - 4 #

[
A_changer
]
^(.+)er$ -> $1 {stamm-pr1} 	# chang(e)
^(.+)r$ -> $1 {stamm-pr4} 	# change(ais), change(ai)

[
A_commencer
]
^(.+)er$ -> $1 {stamm-pr1} 		# commenc(e)
^(.+)cer$ -> $1ç {stamm-pr4} 	# commenç(ons)


##### 3 Präsensstämme #####

# Opposition 1,2,3,6 - 4 - 5 #

[
A_assieger
]
^(.+)é(.)er$ -> $1è$2 {stamm-pr1} 	# assièg(e)
